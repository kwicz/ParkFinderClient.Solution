@{
    ViewData["Title"] = "Home Page";
}

<h1>New Park:</h1>

<form action="/parks" method="post">
  <label for="name">Name</label>
  <input id="name" name="name" type="text">
  <label for="location">Location</label>
  <input id="location" name="location" type="text">
  <label for="dateEstablished">Date Established</label>
  <input id="dateEstablished" name="dateEstablished" type="text">
  <label for="area">Area</label>
  <input id="area" name="area" type="number">
  <label for="description">Description</label>
  <input id="description" name="description" type="text">
  <button type="submit">Go!</button>
</form>

<h1>All Parks:</h1>

<ol>
@foreach (Park park in Model)
{
    <ul>
        <li>ID: @park.ParkId</li>
        <li>Name: @park.Name</li>
        <li>Location: @park.Location</li>
        <li>Date Established: @park.DateEstablished</li>
        <li>Area: @park.Area</li>
        <li>Description: @park.Description</li>
    </ul>
    <a href="/Parks/Details/@(park.ParkId)"><p>View Details</p></a>
    <br>
}
</ol>